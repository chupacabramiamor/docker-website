server {
    listen 80;
    server_name ${DN};
    client_max_body_size 10m;

    location / {
        proxy_pass http://127.0.0.1:${HTTP_PORT};
        proxy_set_header HOST $host;

        proxy_hide_header 'Access-Control-Allow-Origin';
        add_header 'Access-Control-Allow-Origin' '*' always;
    }
}
